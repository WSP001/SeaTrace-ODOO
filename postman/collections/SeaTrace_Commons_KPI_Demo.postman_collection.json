      "script": {
        "type": "text/javascript",
        "exec": [
          "// Global test script for ALL requests in this collection",
          "",
          "// Log response summary",
          "console.log('‚úÖ Response:', pm.response.code, '|', pm.response.responseTime + 'ms');",
          "",
