  "docs": [
    "CODE.AGENT.md",
    "PROJECT.AGENT.md",
    "PROJECT_MANIFEST.json",
    "README.md",
    "CONTRIBUTING.md"
  ],
  "security": {
    "publicKeysOnly": true,
    "secretsManagement": "environment variables (never commit)",
    "auditLogging": true,
    "preCommitHooks": "planned"
  },
  "dependencies": {
    "python": "3.11+",
    "framework": "FastAPI",
    "database": "PostgreSQL",
    "cache": "Redis (optional)",
    "monitoring": "Prometheus + Grafana (planned)"
  },
  "relatedRepos": {
    "private": {
      "name": "SeaTrace003",
      "url": "https://github.com/WSP001/SeaTrace003",
