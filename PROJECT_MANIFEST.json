    },
    "dev": {
      "name": "SeaTrace002",
      "url": "https://github.com/WSP001/SeaTrace002",
      "description": "Development/legacy repository"
    }
  },
  "commonsCharter": {
    "freeTier": {
      "name": "PUL (Public Unlimited License)",
      "features": [
        "Unlimited vessel tracking",
        "Unlimited catch recording",
        "Basic cold storage tracking",
        "Community support"
      ],
      "restrictions": [
        "No advanced analytics",
        "No EMR metering",
        "No priority support"
      ]
    },
    "revenueSharingPercentage": "10-15%",
    "transparencyEndpoint": "/api/commons/fund",
    "publicAccountability": true,
    "reportingUrl": "https://worldseafoodproducers.com/commons/report"
  },
  "licensing": {
    "model": "Commons Charter + PUL",
    "verification": "JWK/JWS (public keys in this repo)",
    "signing": "Ed25519 (private keys in SeaTrace003)",
    "revocation": "Bloom filter CRL (10,000x faster)"
  }
