      "tier": "free"
    },
    {
      "name": "deckside",
      "port": 8001,
      "description": "Catch recording and species tracking (PUBLIC)",
      "entry": "src.deckside.main:app",
      "tier": "free"
    },
    {
      "name": "dockside",
      "port": 8002,
      "description": "Cold storage and port logistics (PUBLIC)",
      "entry": "src.dockside.main:app",
      "tier": "free"
    }
