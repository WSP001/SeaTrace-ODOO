{
  "version": "1.0.0",
  "project": "SeaTrace-ODOO",
  "type": "public",
  "description": "Maritime tracking platform - Commons Charter implementation",
  "website": "https://worldseafoodproducers.com",
  "paths": {
    "root": "C:/Users/Roberto002/Documents/GitHub/SeaTrace-ODOO",
    "src": "src",
    "tests": "tests",
    "docs": "docs",
    "scripts": "scripts"
  },
  "services": [
    {
      "name": "seaside",
      "port": 8000,
      "description": "Vessel tracking and quota management (PUBLIC)",
